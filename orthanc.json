{
	// Sets the locations we can access the DICOM data from. 
	"DicomModalities" : {
		"sample" : [ "STORESCP", "127.0.0.1", 2000 ]
	},

	// Uses environment variables to define username and password from the
	// compose file.
	"RegisteredUsers" : {
		"user" : "password"
	},

	// Allows us to access the server in a container.
	"RemoteAccessAllowed" : true,
	"AuthenticationEnabled" : true,

	// Prevents orthanc from stalling because we have requested too many images
	"LimitFindResults" : 100,
	"LimitFindInstances" : 100
}
